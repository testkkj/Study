package egovframework.example.sample.service.impl.kim;

import org.springframework.stereotype.Repository;

import egovframework.example.sample.service.kim.kimloginVO;
import egovframework.rte.psl.dataaccess.EgovAbstractDAO;

@Repository("kimloginDAO")
public class kimloginDAO extends EgovAbstractDAO {
	public kimloginVO selectSample(kimloginVO kimloginVO) throws Exception {
		return (kimloginVO) select("kimloginVO.selectSample", kimloginVO);
	}
}
